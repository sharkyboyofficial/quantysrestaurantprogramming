{
	"name": "roblox-ts-game",
	"tree": {
		"$className": "DataModel",
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"TS": {
				"$path": "ROBLOXLua/server"
			}
		},

		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$path": "node_modules/@rbxts"
				}
			},
			"Modules": {
				"$path": "ROBLOXLua/shared"
			}
		},

		"StaterGui": {
			"$className": "StarterGui", 
			"StarterGuiScripts": {
				"$path": "ROBLOXLua/startergui"
			}
		},

		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"StarterPlayer": {
					"$path": "ROBLOXLua/client"
				}
			}
		},

		"Workspace": {
			"$className": "Workspace",
			"$properties": {
				"FilteringEnabled": true
			}
		},	

		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},

		"SoundService": {
			"$className": "SoundService",
			"$properties": {
				"RespectFilteringEnabled": true
			}
		}
	}
}
